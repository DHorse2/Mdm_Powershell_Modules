    // "script": "& .\\DevEnv_Module_Reset; & ${fileBasenameNoExtension}",
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Script Verbose",
            "type": "PowerShell",
            "request": "launch",
            "script": " . .\\${fileBasename}",
            "args": [
                "-Verbose"
            ],
            "cwd": "${file}"
        },
        {
            "name": "Script (current)",
            "type": "PowerShell",
            "request": "launch",
            "script": " . .\\${fileBasenameNoExtension}.ps1",
            "args": [],
            "cwd": "${file}"
        },
        {
            "name": "Function Verbose",
            "type": "PowerShell",
            "request": "launch",
            "script": "${fileBasenameNoExtension} -Verbose",
            "args": [],
            "cwd": "${file}"
        },
        {
            "name": "Function (current file)",
            "type": "PowerShell",
            "request": "launch",
            "script": "${fileBasenameNoExtension}",
            "args": [],
            "cwd": "${file}"
        },
        {
            "name": "Launch Functioh set in config",
            "type": "PowerShell",
            "request": "launch",
            "script": "Search-StringInFiles -SearchString 'Author'",
            "cwd": "${cwd}"
        },
        {
            "name": "Attach to PowerShell Host Process",
            "type": "PowerShell",
            "request": "attach",
            // "processId": "${command:PickPSHostProcess}",
            "runspaceId": 1
        },
        {
            "name": "Interactive PowerShell Session",
            "type": "PowerShell",
            "request": "launch",
            "cwd": "${workspaceRoot}"
        }
    ]
}