{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    // "script": "& .\\DevEnv_Module_Reset; & ${fileBasenameNoExtension}",
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Bootstrap PowerShell Function (current file)",
            "type": "PowerShell",
            "request": "launch",
            "script": "${fileBasenameNoExtension}",
            "args": [],
            "cwd": "${file}"
        },
        {
            "name": "Script Launch PowerShell (current)",
            "type": "PowerShell",
            "request": "launch",
            "script": " . .\\${fileBasenameNoExtension}.ps1",
            "args": [],
            "cwd": "${file}"
        },
        {
            "name": "Function Launch PowerShell (current file)",
            "type": "PowerShell",
            "request": "launch",
            "script": "${fileBasenameNoExtension}",
            "args": [],
            "cwd": "${file}"
        },
        {
            "name": "Prompt for Launch Script PowerShell",
            "type": "PowerShell",
            "request": "launch",
            "script": "Enter path or command to execute, for example, ${workspaceFolder}/src/foo.ps1 or Invoke-Pester",
            "cwd": "${cwd}"
        },
        {
            "name": "Attach to PowerShell Host Process",
            "type": "PowerShell",
            "request": "attach",
            // "processId": "${command:PickPSHostProcess}",
            "runspaceId": 1
        },
        {
            "name": "Interactive PowerShell Session",
            "type": "PowerShell",
            "request": "launch",
            "cwd": "${workspaceRoot}"
        }
    ]
}